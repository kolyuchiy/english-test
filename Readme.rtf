\par procedure randombitmap;
\par var
\par    srec: SearchRec;
\par    i: word;
\par begin
\par    i:=0;
\par    findfirst(PicsPath+'\\*.bmp',AnyFile,srec);
\par    while DosError=0 do begin
\par       inc(i);
\par       findnext(srec);
\par    end;
\par    i:=random(i)+1;
\par    findfirst(PicsPath+'\\*.bmp',AnyFile,srec);
\par    while (DosError=0) and (i<>0) do begin
\par       dec(i);
\par       findnext(srec);
\par    end;
\par    showbmp(PicsPath+'\\'+srec.name);
\par end;
