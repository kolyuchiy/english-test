\par procedure drawbutton(x,y,xdim,ydim: word;
\par                      caption: string;
\par                      active: boolean);
\par const
\par    col      = LightCyan;   acol      = LightRed;
\par    shadecol = Cyan;        ashadecol = Red;
\par    textcol  = Black;       atextcol  = White;
\par begin
\par    setlinestyle(SolidLn,0,ThickWidth);
\par    setfillstyle(SolidFill,LightGray);
\par    bar(x,y,x+xdim,y+ydim);
\par    if active then setcolor(acol)
\par    else setcolor(col);
\par    line(x,y,x+xdim,y);
\par    line(x,y,x,y+ydim);
\par    if active then setcolor(ashadecol)
\par    else setcolor(shadecol);
\par    line(x+xdim,y+ydim,x+xdim,y);
\par    line(x+xdim,y+ydim,x,y+ydim);
\par    if active then setcolor(atextcol)
\par    else setcolor(textcol);
\par    settextstyle(DefaultFont,HorizDir,0);
\par    outtextxy (x+(xdim-textwidth(caption)) div 2,
\par               y+(ydim-textheight(caption)) div 2,
\par               caption);
\par end;
\par 
\par procedure StatusLine(st: string);
\par const
\par    x = 10;  y = 455;
\par    dx = 10;
\par    xdim = 620; ydim = 16;
\par    clear = '%clear%';
\par begin
\par    setviewport(x,y,x+xdim,y+ydim,true);
\par    clearviewport;
\par    if st<>clear then begin
\par       setlinestyle(SolidLn,0,NormWidth);
\par       setcolor(LightGray);
\par       line(xdim,ydim,0,ydim);
\par       line(xdim,ydim,xdim,0);
\par       setcolor(DarkGray);
\par       line(0,0,0,ydim);
\par       line(0,0,xdim,0);
\par       setcolor(White);
\par       settextstyle(SmallFont, HorizDir, 0);
\par       outtextxy(dx, (ydim - textheight(st)) div 2, st);
\par    end;
\par    setviewport(0,0,getmaxx,getmaxy,false);
\par end;
\par 
\par function menu (count: integer;
\par                const values,captions: array of string;
\par                x,y,xdim,ydim,time: word): string;
\par const
\par    kUp = #72;
\par    kDown = #80;
\par    kEnter = #13;
\par    kEsc = #27;
\par    skipbutton = 4;
\par    skipempty = 10;
\par var
\par    cy,active: integer;
\par    time0,timec,timelast: longint;
\par    i,hour,min,sec: word;
\par    st: string;
\par begin
\par    active:=0;
\par    while (captions[active]='') and (active<count) do inc(active);
\par    if time<>0 then begin
\par       gettime(hour,min,sec,i);
\par       time0:=hour*60*60+min*60+sec;
\par       timelast:=time0;
\par    end;
\par    repeat
\par       cy:=y;
\par       for i:=0 to count-1 do
\par          if captions[i]<>'' then begin
\par             drawbutton(x, cy, xdim, ydim, captions[i], i = active);
\par             cy:=cy+(ydim+skipbutton);
\par          end else
\par             cy:=cy+skipempty;
\par       if time<>0 then
\par          repeat
\par             gettime(hour,min,sec,i);
\par             timec:=hour*60*60+min*60+sec;
\par             if (timec-time0)>=time then begin
\par                menu:='';
\par                exit;
\par             end;
\par             if timec-timelast = 2 then begin
\par                str(time-(timec-time0),st);
\par                setcolor(White);
\par                settextstyle(SmallFont,HorizDir,0);
\par                StatusLine(lng.status_test+' - '+
\par                           lng.time_left+' '+st+' '+lng.sec);
\par                timelast:=timec;
\par             end;
\par          until keypressed;
\par       case readkey of
\par          kUp:    repeat
\par                     dec(active);
\par                  until (captions[active]<>'') and (active<count);
\par          kDown:  repeat
\par                     inc(active);
\par                  until (captions[active]<>'') and (active>=0);
\par          kEsc:   begin
\par                     menu:=ESCAPE;
\par                     exit;
\par                  end;
\par          kEnter: begin
\par                     menu:=values[active];
\par                     exit;
\par                  end;
\par       end;
\par       if active<0 then begin
\par          active:=count-1;
\par          while (captions[active]='') and (active>=0) do dec(active);
\par       end
\par       else if active>count-1 then begin
\par          active:=0;
\par          while (captions[active]='') and (active<count) do inc(active);
\par       end;
\par    until false;
\par end;
\par 
\par procedure test(name: string);
\par const
\par    tests = 4;
\par    txdim = 500; ty = 80;
\par    lnskip = 10;
\par    right = '%right%';
\par    question = '??';
\par    comments = '#';
\par var
\par    f,fTest: text;
\par    captions,values: array[1..tests] of string;
\par    st,tmpst,ans: string;
\par    capts,cstr,rand,i,bottomy: word;
\par    rightans, quest: real;
\par    month,day,hour,minute: word;
\par label
\par    end_of_test;
\par begin
\par    assign(fTest, name);
\par    reset(fTest);
\par    if IOResult<>0 then begin
\par       closegraph;
\par       writeln(lng.file_not_found);
\par       halt;
\par    end;
\par    rightans:=0; quest:=0;
\par    repeat
\par       setcolor(White);
\par       cleardevice;
\par       randombitmap;
\par       capts:=1; cstr:=0;
\par       repeat
\par          if eof(fTest) then goto END_OF_TEST;
\par          readln(fTest,st);
\par          st:=decrypt(st);
\par          if pos(comments,st)=1 then continue
\par          else if pos(question,st)=1 then
\par          begin
\par             captions[capts]:=copy(st,1+length(question),
\par                                   length(st)-length(question));
\par             inc(capts);
\par             if capts>tests then break;
\par          end else
\par          begin
\par             settextstyle(0,0,0);
\par             outtextxy((getmaxx-txdim) div 2 + 5,
\par                       5+ty+cstr*(textheight(st)+lnskip),st);
\par             inc(cstr);
\par          end;
\par       until false;
\par       setlinestyle(SolidLn,0,NormWidth);
\par       bottomy:=ty + (textheight(st)+lnskip)*cstr;
\par       rectangle((getmaxx-txdim) div 2, ty,
\par                 (getmaxx-txdim) div 2 + txdim, bottomy);
\par       StatusLine(lng.status_test);
\par 
\par       rand:=random(4)+1;
\par       for i:=1 to tests do values[i]:='';
\par       values[rand]:=right;
\par       tmpst:=captions[1];
\par       captions[1]:=captions[rand];
\par       captions[rand]:=tmpst;
\par       ans:=menu(tests,values,captions,
\par                 (getmaxx-230) div 2,bottomy+50,230,30,60);
\par       quest:=quest+1;
\par       if ans=right then rightans:=rightans+1;
\par       if ans=ESCAPE then begin
\par          setcolor(White);
\par          cleardevice;
\par          randombitmap;
\par          goto END_OF_TEST;
\par       end;
\par    until false;
\par END_OF_TEST:
\par    close(fTest);
\par 
\par    assign(f,fResults);
\par    append(f);
\par    if IOResult<>0 then rewrite(f);
\par    writeln(f);
\par    getdate(i,month,day,i);
\par    gettime(hour,minute,i,i);
\par    writeln(f,day,'.',month,' ',hour,':',minute);
\par    setlinestyle(SolidLn,0,NormWidth);
\par    rectangle (90, 90, 300, 170);
\par    settextstyle(DefaultFont,HorizDir,0);
\par    str(trunc(rightans),st);
\par    str(trunc((rightans/quest)*100),tmpst);
\par    outtextxy(100,100,lng.results_right+st+' ('+tmpst+'%)');
\par    writeln(f,lng.results_right+st+' ('+tmpst+'%)');
\par    str(trunc(quest),st);
\par    outtextxy(100,150,lng.results_all+st);
\par    writeln(f,lng.results_all+st);
\par    StatusLine(lng.status_text);
\par    close(f);
\par    readkey;
\par end;
\par 
\par procedure TextFile(fname: string);
\par const
\par    x = 100;    y = 100;
\par    xdim = 400; ydim = 200;
\par var
\par    f: text;
\par    s: string;
\par    cy: word;
\par label
\par    end_of_proc;
\par begin
\par       setviewport(x,y,x+xdim,y+ydim,true);
\par       clearviewport;
\par       setlinestyle(SolidLn,0,NormWidth);
\par       setcolor(DarkGray);
\par       line(xdim,ydim,0,ydim);
\par       line(xdim,ydim,xdim,0);
\par       setcolor(LightGray);
\par       line(0,0,0,ydim);
\par       line(0,0,xdim,0);
\par       setcolor(White);
\par       settextstyle(SmallFont, HorizDir, 0);
\par       assign(f, fname);
\par       reset(f);
\par       cy:=10;
\par       if IOResult<>0 then begin
\par          outtextxy(20,cy, lng.file_not_found);
\par          goto end_of_proc;
\par       end else
\par       while not eof(f) do begin
\par         readln(f,s);
\par         outtextxy(20, cy, s);
\par         inc(cy,textheight(s)+2);
\par       end;
\par       close(f);
\par end_of_proc:
\par       StatusLine(lng.status_text);
\par       readkey;
\par       setviewport(0,0,getmaxx,getmaxy,false);
\par       cleardevice;
\par end;
\par 
\par procedure GraphDrv; external; \{$L egavga.obj\}
\par procedure LittFont; external; \{$L litt.obj\}
\par procedure SansFont; external; \{$L sans.obj\}
\par 
\par begin
\par    randomize;
\par    lng_read(lng);
